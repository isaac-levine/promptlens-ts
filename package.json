{
  "name": "promptlens",
  "version": "0.1.6",
  "description": "TypeScript SDK for working with PromptLens - optimize and monitor your AI prompts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node ./node_modules/.bin/jest",
    "lint": "node ./node_modules/.bin/eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "ai",
    "llm",
    "prompt",
    "engineering",
    "testing",
    "monitoring",
    "optimization",
    "openai",
    "gpt",
    "experiment"
  ],
  "author": "PromptLens <hello@promptlens.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaac-levine/promptlens-ts.git"
  },
  "bugs": {
    "url": "https://github.com/isaac-levine/promptlens-ts/issues"
  },
  "homepage": "https://github.com/isaac-levine/promptlens-ts#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "jest": "^29.7.0",
    "openai": "^4.87.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/express": "^5.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "prisma": "^6.5.0"
  }
}
